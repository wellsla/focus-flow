# Auth0 Configuration (Required for authentication)
AUTH0_SECRET=your_random_32_char_secret_here
AUTH0_ISSUER_BASE_URL=https://your-tenant.auth0.com
AUTH0_CLIENT_ID=your_auth0_client_id
AUTH0_CLIENT_SECRET=your_auth0_client_secret
AUTH0_BASE_URL=http://localhost:9002

# Google Gemini API (Required for AI features)
GEMINI_API_KEY=your_gemini_api_key_here

# Optional Feature Flags
# NEXT_PUBLIC_FEATURE_SHELL_SAFE=true  # Enable minimal shell for debugging
# NEXT_PUBLIC_ENABLE_MOTIVATIONAL_HEADER=false  # Disable rotating quotes
# NEXT_PUBLIC_REACT_COMPILER=off  # Disable React Compiler
